{
    "name": "spa-vite-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:msw": "VITE_ENABLE_MSW=true vite",
        "build": "tsc && vite build",
        "test": "vitest",
        "test:ci": "vitest run --reporter verbose --reporter=junit --outputFile=./unit-report.xml",
        "preview": "vite preview",
        "prettier": "prettier . --check",
        "prettier-fix": "prettier . --write",
        "lint": "CI=true eslint --ignore-pattern '*.d.ts' --ignore-pattern '*.test.*' --ext ts,tsx --max-warnings 0 src",
        "prepare": "husky install",
        "storybook": "storybook dev -p 6006",
        "test-storybook": "pnpm exec test-storybook --watch",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@emotion/react": "^11.13.0",
        "@emotion/styled": "^11.13.0",
        "@fontsource/inter": "^4.5.15",
        "@fontsource/roboto": "^5.0.14",
        "@mui/icons-material": "^5.16.7",
        "@mui/material": "^5.16.7",
        "@mui/x-data-grid": "^7.12.1",
        "@tanstack/react-query": "^4.36.1",
        "framer-motion": "^9.1.7",
        "intl-messageformat": "^10.5.14",
        "ky": "1.5.0",
        "lodash-es": "^4.17.21",
        "query-string": "^8.2.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-error-boundary": "^3.1.4",
        "react-hook-form": "^7.52.2",
        "react-router-dom": "^6.25.1",
        "zustand": "^4.5.4"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^1.6.1",
        "@faker-js/faker": "^8.4.1",
        "@mswjs/data": "^0.16.1",
        "@storybook/addon-a11y": "^8.2.9",
        "@storybook/addon-actions": "^8.2.9",
        "@storybook/addon-essentials": "^8.2.9",
        "@storybook/addon-interactions": "^8.2.9",
        "@storybook/addon-links": "^8.2.9",
        "@storybook/react": "^8.2.9",
        "@storybook/react-vite": "^8.2.9",
        "@storybook/test": "^8.2.9",
        "@storybook/test-runner": "^0.19.1",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.4.8",
        "@testing-library/react": "^16.0.0",
        "@testing-library/user-event": "^14.5.2",
        "@trivago/prettier-plugin-sort-imports": "4.1.1",
        "@types/lodash-es": "^4.17.12",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/testing-library__jest-dom": "^5.14.9",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "axe-playwright": "^1.2.3",
        "concurrently": "8.2.0",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-storybook": "^0.8.0",
        "http-server": "^14.1.1",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jsdom": "^21.1.2",
        "msw": "^2.3.5",
        "msw-storybook-addon": "^2.0.3",
        "playwright": "^1.45.2",
        "prettier": "2.8.6",
        "pretty-quick": "^3.3.1",
        "storybook": "^8.2.9",
        "storybook-addon-remix-react-router": "^3.0.0",
        "storybook-dark-mode": "^4.0.2",
        "typescript": "^5.3.3",
        "vite": "^5.3.4",
        "vite-plugin-checker": "^0.6.4",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.6.0",
        "wait-on": "^7.2.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "last 2 versions"
        ],
        "development": [
            "last 1 version"
        ]
    },
    "msw": {
        "workerDirectory": "public"
    },
    "packageManager": "pnpm@9.6.0"
}
